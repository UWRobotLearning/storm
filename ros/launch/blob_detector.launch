  <!-- <param name="panda_tray" textfile="/home/mohak/workspace/storm_rl/content/assets/urdfs/panda/panda_arm_tray_robot.urdf"/> -->
  <!-- <include file="$(find franka_mpc_utils)/launch/static_transform_publisher_panda_2.launch" pass_all_args="true"/> -->

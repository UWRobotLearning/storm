  </link>
  <link name="wall_3">
    <visual>
      <origin xyz="0 0.1225 0.02" rpy="0 0 0" />
      <geometry>
        <box size="0.25 0.005 0.03" />
      </geometry>
      <material name="tray_material" />
    </visual>
    <collision>
      <origin xyz="0 0.1225 0.02" rpy="0 0 0" />
      <geometry>
        <box size="0.25 0.005 0.03" />
      </geometry>
      <material name="tray_material">

  </link>
  <link name="wall_2">
    <visual>
      <origin xyz="-0.1225 0 0.02" rpy="0 0 0" />
      <geometry>
        <box size="0.005 0.25 0.03" />
      </geometry>
      <material name="tray_material" />
    </visual>
    <collision>
      <origin xyz="-0.1225 0 0.02" rpy="0 0 0" />
      <geometry>
        <box size="0.005 0.25 0.03" />
      </geometry>
      <material name="tray_material">
        <color rgba="0.8 0.8 0.8 1.0" />
      </material>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.05" ixy="0.0" ixz="0.0" iyy="0.05" iyz="0.0" izz="0.05"/>
